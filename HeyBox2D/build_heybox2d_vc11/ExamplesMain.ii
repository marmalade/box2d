struct ScissorRect
{
    union {
        uint32 xy32;
        struct {
            uint16 x;
            uint16 y;
        } ;
    } ;
    union {
        uint32 wh32;
        struct {
            uint16 w;
            uint16 h;
        } ;
    } ;

    bool IsEqual(ScissorRect* s){return ( s->xy32 == xy32 && s->wh32 == wh32);}

    bool operator==( const ScissorRect & src ) const
    {
        if ( this == &src )
        {
            return true;
        }

        return ( ( xy32 == src.xy32 ) && ( wh32 == src.wh32 ) );
    }

    bool operator!=( const ScissorRect & src ) const
    {
        return !( *this == src );
    }
};


    else
    {
        clipPos.x = _IW_GX_CLIPSCALEVIEW_MUL(viewPos.x, pState->m_XFactor);
        clipPos.y = _IW_GX_CLIPSCALEVIEW_MUL(viewPos.y, pState->m_YFactor);
        clipPos.z = _IW_GX_CLIPSCALEVIEW_MUL(viewPos.z, pState->m_ZFactor);
    }
}

